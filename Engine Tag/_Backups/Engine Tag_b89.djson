ible":true,"Width":125.0,"Name":"txt_carName","RenderingSkip":0,"MinimumRefreshIntervalMS":0.0},{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.TextItem, SimHub.Plugins","IsTextItem":true,"Font":"Microsoft YaHei","TextPadding":{"PaddingRight":12},"FontStyle":"Italic","FontSize":33.0,"Text":"非常激进","TextColor":"#FFF5F5F5","HorizontalAlignment":1,"VerticalAlignment":1,"BackgroundColor":"#00FFFFFF","Height":45.0,"Left":80.0,"Top":30.0,"Visible":true,"BlinkDelay":75.0,"Width":175.0,"Name":"TextItem2","RenderingSkip":50,"MinimumRefreshIntervalMS":500.0,"Bindings":{"Text":{"Formula":{"JSExt":3,"Interpreter":1,"Expression":"// engineMapReader.js\r\n\r\nconst fs = require('fs');\r\n\r\n/**\r\n * 从 JSON 文件读取汽车数据，并返回指定模式对应的 ENG_map\r\n * @param {number} mode - 要查询的引擎模式编号\r\n * @param {string} [jsonFile='car_data.json'] - JSON 文件名\r\n * @returns {Object} 对应的引擎映射数据\r\n * @throws {Error} 如果文件读取失败或模式不存在\r\n */\r\nfunction getEngineMap(mode, jsonFile = 'lib_engineMapComments.json') {\r\n    try {\r\n        // 同步读取 JSON 文件\r\n        const rawData = fs.readFileSync(jsonFile);\r\n        const carData = JSON.parse(rawData);\r\n        \r\n        // 查找指定模式的 ENG_map\r\n        if (carData.engine && carData.engine.modes) {\r\n            const engineMode = carData.engine.modes.find(m => m.mode === mode);\r\n            if (engineMode && engineMode.ENG_map) {\r\n                return engineMode.ENG_map;\r\n            }\r\n        }\r\n        \r\n        throw new Error(`Engine mode ${mode} not found`);\r\n    } catch (err) {\r\n        throw new Error(`Failed to get engine map: ${err.message}`);\r\n    }\r\n}"},"Mode":2}}}],"MinimumRefreshIntervalMS":0.0}],"SnapToGrid":true,"HideLabels":false,"ShowForeground":true,"ForegroundOpacity":100.0,"ShowBackground":true,"BackgroundOpacity":100.0,"ShowBoundingRectangles":true,"GridSize":5,"Images":[],"Metadata":{"ScreenCount":1.0,"InGameScreensIndexs":[0],"IdleScreensIndexs":[0],"MainPreviewIndex":0,"IsOverlay":true,"OverlaySizeWarning":false,"MetadataVersion":2.0,"EnableOnDashboardMessaging":true,"PitScreensIndexs":[],"SimHubVersion":"9.8.2","Title":"Engine Tag","Width":300.0,"Height":100.0,"DashboardVersion":""},"ShowOnScreenControls":true,"IsOverlay":true,"EnableClickThroughOverlay":true,"EnableOnDashboardMessaging":true,"UseStrictJSIsolation":false,"UseStrictJSIsolationWarning":true}